{
    "manifest_version": 3,
    "name": "Mi Extensión de Debugging",
    "version": "1.0",
    "description": "Una extensión para demostrar el debugging",
    "permissions": ["activeTab",    "tabCapture", "audioCapture"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "devtools_page": "devtools.html"
  }